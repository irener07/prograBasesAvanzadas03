<div class="row">
<a href="/supermarkets/createSupermarket" class="btn btn-success btn-block btn-sm">New Supermarket</a>
<table class="egt table {
    border-collapse: separate;
    border-spacing: 5px;
    background: #000 url("gradient.gif") bottom left repeat-x;
    color: #fff;
 }">
   
  <tr>
    <th scope="row">ID</th>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Image</th>
    <th>Telephone</th>
    <th>Rating</th>
    <th>Schedule</th>
    <th>Website</th>
    <th>Products</th>
    <th></th>
    <th></th>
  </tr>
  
    {{#each supermarketsFound}}
    latitude, image, telephone, rating, schedule, website, products

  <tr>
    <th>{{idSuperMarket}}</th>
    <td>{{name}}</td>
    <td>{{description}}</td>
    <td>{{address}}</td>
    <td>{{longitude}}</td>
    <td>{{latitude}}</td>
    <td>{{image}}</td>
    <td>{{telephone}}</td>
    <td>{{rating}}</td>
    <td>{{schedule}}</td>
    <td>{{website}}</td>
    <td>{{products}}</td>
    
  </tr>
        {{else}}
            <div class="card mx-auto">
                <div class="card-body">
                    <h4 class="lead">No supermarkets have been found.</h4>
                    <a href="/supermarkets/createSupermarket" class="btn btn-success btn-block">Register a New Supermarket</a>
                </div> 
            </div>
    {{/each}}
    </table>
</div>

